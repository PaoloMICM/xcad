{
  "$schema": "http://json.schemastore.org/template",
  "author": "Xarial",
  "classifications": [ "xCAD", "SOLIDWORKS", "Inventor", "Console" ],
  "identity": "Xarial.XCad.Template.Console.CSharp",
  "groupIdentity": "Xarial.XCad.Template.Console",
  "name": "xCAD Console",
  "shortName": "xcadconsole",
  "sourceName": "__TemplateNamePlaceholder__",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "defaultName": "XCadSample",
  "preferNameDirectory": true,
  "symbols": {
    "_TargetFramework_": {
      "type": "parameter",
      "displayName": "Target Framework",
      "description": "Project target framework",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net461",
          "displayName": ".NET Framework 4.6.1"
        },
        {
          "choice": "net472",
          "displayName": ".NET Framework 4.7.2"
        },
        {
          "choice": "net48",
          "displayName": ".NET Framework 4.8"
        },
        {
          "choice": "netcoreapp3.1",
          "displayName": ".NET Core 3.1"
        },
        {
          "choice": "net6.0-windows",
          "displayName": ".NET 6"
        },
        {
          "choice": "net7.0-windows",
          "displayName": ".NET 7"
        }
      ],
      "defaultValue": "net472",
      "replaces": "net472"
    },
    "_SolidWorks_": {
      "type": "parameter",
      "displayName": "SOLIDWORKS",
      "description": "Adds the SOLIDWORKS implementation1",
      "datatype": "bool",
      "defaultValue": "true"
    },
    "_SwDocumentManager_": {
      "type": "parameter",
      "displayName": "SOLIDWORKS Document Manager",
      "description": "Adds the SOLIDWORKS implementation",
      "datatype": "bool",
      "defaultValue": "true"
    },
    "_Inventor_": {
      "type": "parameter",
      "displayName": "Autodesk Inventor",
      "description": "Adds the Autodesk Inventor implementation",
      "datatype": "bool",
      "defaultValue": "false"
    }
  },
  "sources": [
    {
      "modifiers": [
        {
          "condition": "!(_SolidWorks_)",
          "exclude": [
            "__TemplateNamePlaceholder__Console.Sw/**"
          ]
        },
        {
          "condition": "!(_SwDocumentManager_)",
          "exclude": [
            "__TemplateNamePlaceholder__Console.SwDm/**"
          ]
        },
        {
          "condition": "!(_Inventor_)",
          "exclude": [
            "__TemplateNamePlaceholder__Console.Ai/**"
          ]
        }
      ]
    }
  ],
  "postActions": [{
    "condition": "(!skipRestore)",
    "description": "Restore NuGet packages required by this project.",
    "actionId": "210D431B-A78B-4D2F-B762-4ED3E3EA9025",
    "continueOnError": true
  }]
}