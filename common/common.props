<Project>
  <PropertyGroup>
    <OutputPath>$(SolutionDir)_build\</OutputPath>
    <Product>xCAD</Product>
    <BuildNumber>0</BuildNumber>
    <VersionPrefix>0.8.$(BuildNumber)</VersionPrefix>
    <Company>Xarial Pty Limited</Company>
    <Copyright>Copyright Â© Xarial Pty Limited 2022</Copyright>
    <Authors>Xarial</Authors>
    <PackageProjectUrl>https://xcad.net</PackageProjectUrl>
    <RepositoryUrl>https://github.com/xarial/xcad</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <SignAssembly>false</SignAssembly>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <AssemblyOriginatorKeyFile></AssemblyOriginatorKeyFile>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>
  <PropertyGroup Condition="'$(TF_BUILD)' == 'true'">
    <ContinuousIntegrationBuild>true</ContinuousIntegrationBuild>
  </PropertyGroup>
</Project>